<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="DogeBarcelo" transaction-type="RESOURCE_LOCAL">
		<class>src.entidades.Cadena_Hotelera</class>
		<class>src.entidades.Cliente</class>
		<class>src.entidades.Controlhorario</class>
		<class>src.entidades.ControlhorarioPK</class>
		<class>src.entidades.Elemento</class>
		<class>src.entidades.Factura</class>
		<class>src.entidades.Habitacione</class>
		<class>src.entidades.Hora</class>
		<class>src.entidades.Hotel</class>
		<class>src.entidades.Pago</class>
		<class>src.entidades.PagoPK</class>
		<class>src.entidades.Permiso</class>
		<class>src.entidades.PermisoPK</class>
		<class>src.entidades.Reservacion</class>
		<class>src.entidades.Servicio</class>
		<class>src.entidades.ServicioExtra</class>
		<class>src.entidades.Shiftreport</class>
		<class>src.entidades.ShiftreportPK</class>
		<class>src.entidades.Usuario</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/dogebarcelo"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.password" value="root"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
			
		</properties>
	</persistence-unit>
</persistence>
